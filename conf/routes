# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
GET     /regi                           controllers.HomeController.regi

GET     /add                   controllers.HomeController.add()
POST    /create                controllers.HomeController.create()

GET     /edit/:taskNo                   controllers.HomeController.edit(taskNo:Int)
POST    /update/:taskNo                controllers.HomeController.update(taskNo:Int)

GET     /delete/:taskNo                   controllers.HomeController.delete(taskNo:Int)
POST    /remove/:taskNo                controllers.HomeController.remove(taskNo:Int)


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
